<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="socialnetwork.socialnetwork.gui.CreateConversationController" prefHeight="600.0" prefWidth="800.0">
    <VBox spacing="10" alignment="CENTER">
        <TextField fx:id="userSearchField" promptText="Search for users"/>
        <ListView fx:id="userListView" prefHeight="200"/>
        <Button text="Create Conversation" onAction="#handleCreateConversation"/>
    </VBox>
</AnchorPane>